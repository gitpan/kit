all::

C_DIRS = badtar btoa
SH_DIRS = kit shar man

NoManPages()
SetSubdirs($(SH_DIRS) $(C_DIRS) des)
DependDirs($(C_DIRS))
AddedByConfigure(install mkdep cppstdin)

